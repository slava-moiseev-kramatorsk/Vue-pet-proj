{"ast":null,"code":"// import axios from \"axios\";\nimport { mapActions, mapGetters } from \"vuex\";\nimport router from \"@/router\";\nimport ShowBook from \"@/components/children/ShowBook\";\nexport default {\n  // eslint-disable-next-line vue/no-unused-components\n  components: {\n    ShowBook\n  },\n\n  data() {\n    return {};\n  },\n\n  name: 'ListPage',\n  computed: { ...mapGetters(['allBooks'])\n  },\n  methods: { ...mapActions(['getAllBooks', 'deleteBook', 'getTest']),\n\n    updateBook(book) {\n      router.push({\n        name: 'editBook',\n        params: {\n          currentBook: book,\n          id: book.id\n        }\n      });\n    },\n\n    deleteItem(book) {\n      this.deleteBook(book);\n      console.log(book.id);\n    },\n\n    viewBook(book) {\n      router.push({\n        name: 'showBook',\n        params: {\n          currentBook: book,\n          id: book.id\n        }\n      });\n    }\n\n  },\n\n  created() {\n    this.getAllBooks();\n    this.getTest();\n  }\n\n};","map":{"version":3,"mappings":"AA8CA;AACA;AACA;AACA;AAEA;EACA;EACAA;IAAAC;EAAA,CAFA;;EAGAC;IACA;EAEA,CANA;;EAOAC,gBAPA;EASAC,YACA;EADA,CATA;EAaAC,WACA,uDADA;;IAEAC;MACAC;QAAAJ;QAAAK;UAAAC;UAAAC;QAAA;MAAA;IACA,CAJA;;IAKAC;MACA;MACAC;IACA,CARA;;IASAC;MACAN;QAAAJ;QAAAK;UAAAC;UAAAC;QAAA;MAAA;IACA;;EAXA,CAbA;;EA2BAI;IACA;IACA;EACA;;AA9BA","names":["components","ShowBook","data","name","computed","methods","updateBook","router","params","currentBook","id","deleteItem","console","viewBook","created"],"sourceRoot":"src/components/children","sources":["ListPage.vue"],"sourcesContent":["<template>\n  <div>\n    <div class=\"header\">\n      <router-link to=\"/\" class=\"link1\">Home</router-link>\n      <div><router-link to=\"/new-book\" class=\"link1\">Добавить</router-link></div>\n    </div>\n    <div >\n      <input  id=\"searching\" class=\"inputSearch\" name=\"findBook\" placeholder=\"Поиск\" v-focus>\n<!--      <el-input name=\"title\" placeholder=\"Ведите название\" v-focus></el-input>-->\n    </div>\n\n    <div v-for=\"book in allBooks\" :key=\"book.id\" class=\"book\">\n      <div class=\"titleBook\">Название: {{book.title}}</div>\n      <div>Жанр: {{book.genre}}</div>\n      <div>Автор: {{book.author}}</div>\n      <div>Страниц: {{book.page}}</div>\n      <div>Цена: {{book.price}}</div>\n      <div>Краткое описание: {{book.description}}</div>\n      <div>\n      <el-button\n          class=\"btn\"\n          type=\"danger\"\n          plain\n          @click=\"deleteItem(book)\"\n      >Удалить</el-button>\n      <el-button\n          class=\"btn\"\n          type=\"primary\"\n          plain\n          @click=\"updateBook(book)\"\n      >Редактировать</el-button>\n        <el-button\n            class=\"btn\"\n            type=\"primary\"\n            plain\n            @click=\"viewBook(book)\"\n        >Просмотр</el-button>\n      </div>\n<!--      <router-link :to=\"{name: 'editBook', params: { book: book, id: book.id }}\"  class=\"link1\">Edit</router-link>-->\n      <hr>\n    </div>\n  </div>\n</template>\n\n<script>\n\n// import axios from \"axios\";\nimport {mapActions, mapGetters} from \"vuex\";\nimport router from \"@/router\";\nimport ShowBook from \"@/components/children/ShowBook\";\n\nexport default {\n  // eslint-disable-next-line vue/no-unused-components\n  components: {ShowBook},\n  data() {\n    return {\n    }\n  },\n  name: 'ListPage',\n\n  computed: {\n    ...mapGetters(['allBooks']),\n  },\n\n  methods: {\n    ...mapActions(['getAllBooks','deleteBook', 'getTest']),\n    updateBook(book){\nrouter.push({name: 'editBook', params: { currentBook: book, id: book.id }})\n    },\n    deleteItem(book){\n      this.deleteBook(book);\n      console.log(book.id)\n    },\n    viewBook(book){\n      router.push({name: 'showBook', params: { currentBook: book, id: book.id }})\n    },\n  },\n\n  created() {\n    this.getAllBooks();\n    this.getTest();\n  }\n\n}\n</script>\n\n<style>\n\n.header{\n  display: flex;\n  flex-direction: row;\n  justify-content: space-around;\n  justify-items: left;\n  align-items: center;\n  width: auto;\n  height: 70px;\n  background-color: steelblue;\n}\n.link1{\n  text-decoration: none;\n  color: aliceblue;\n  font-size: 25px;\n  font-family: fantasy, sans-serif;\n}\n.book{\n  display: flex;\n  flex-direction: column;\n  align-items: flex-start;\n  margin: 10px;\n}\n.titleBook{\n  font-size: 30px;\n  font-family: sans-serif;\n}\n.inputSearch{\n  margin: 15px;\n  width: 300px;\n  height: 20px;\n}\n</style>"]},"metadata":{},"sourceType":"module"}