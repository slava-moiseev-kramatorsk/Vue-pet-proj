pActions } from 'vuex'\n\n\n\nexport default {\n  name: 'MainPage',\n  components: {\n    AllBooks,\n  },\n  computed:{\n    ...mapGetters(['allBooks']),\n    childreBooks(){\n      return this.$store.getters.childrenBooks()\n    },\n    advantureBooks(){\n      return this.$store.getters.advantureBooks()\n    },\n    horrorbooks() {\n      return this.$store.getters.horrorbooks()\n    }\n  },\n  methods:{\n    ...mapActions(['updateBooks']),\n\n    sync(){\n      db.ref('books').once(\n          'value',\n          snapshot => {\n            const response = snapshot.val()\n            this.updateBooks(response);\n          }\n      )\n    },\n    update(){\n      db.ref('books').once(\n          'value',\n          snapshot => {\n            const response = snapshot.val()\n            this.updateBooks(response);\n            console.dir(response)\n          }\n      )\n    }\n  },\n}\n\n</script>\n\n\n<style lang=\"scss\">\n\n.parent{\n  height: 100vh;\n  width: 100vw;\n  //display: flex;\n  //flex-direction: column;\n  //align-items: center;\n  //justify-content: center;\n\n  .content{\n    font-size: 40px;\n    color: #00f;\n  }\n\n  .btn{\n    height: 40px;\n\n    margin-top: 20px;\n\n    &.btn-first{\n\n    }\n    &.btn-second{\n      margin-top: 30px;\n    }\n  }\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}